Label: 
Page.Label({ -- adds a small text box
    Text = "example"
})

Button:
Page.Button({ -- is a button
    Text = "Click Me!",
    Callback = function()
        print("Clicked!")    
    end
})

Toggle:
Page.Toggle({ -- toggles a script, on or off
    Text = "On or Off",
    Callback = function(value)
        print(value)
    end,
    Enabled = true
})

Pages:
local Page = UI.New({
    Title = "Main"
})

local Page2 = UI.New({ -- change number after page to make new pages ex: Page3, Page4
    Title = "New Page"
})

Slider:
Page.Slider({ -- is a slider
    Text = "Slide Me!",
    Callback = function(value)
        print(value)
    end,
    Min = 0,
    Max = 10,
    Def = 4
})

Dropdown:
Page.Dropdown({ -- gives several options to pick from
    Text = "Choose an option",
    Callback = function(value)
        print(value)
    end,
    Options = {"a", "b", "c"}
})

TextField:
Page.TextField({ -- you can type things in this
    Text = "Name",
    Callback = function(value)
        print(value)
    end
})

DataField:
Page.DataTable({ -- basically a dropdown
    Text = "DataTable",
    Callback = function(data)
        table.foreach(data, print)    
    end,
    Options = {
        A = true,
        B = true,
        C = false
    }
})

Color Picker:
Page.ColorPicker({ -- picks a color
    Text = "Choose A Color",
    Default = Color3.fromRGB(0, 255, 0),
    Callback = function(value)
        print(value)
    end
})

Chipset:
Page.ChipSet({
    Text = "ChipSet",
    Callback = function(chipset)
        table.foreach(chipset, print)    
    end,
    Options = {
        A = true,
        B = true,
        C = false
    }
})

New UI:
local UI = BooHub.Load({ -- basically a dropdown
     Title = "Hello World!",
     Style = 3,
     SizeX = 400,
     SizeY = 200,
     Theme = "Light"
})

Multiple Game Support: -- lets you run a script on several different games on one loadstring
elseif game.PlaceId == Game ID here then
    local BooHub = loadstring(game:HttpGet("GitHub or Pastebin link here"))()
   local PlaceId = game.PlaceId
   
   -- If you already have an elseif, do an if, then do elseif, this repeats itself
Anti-AFK:
wait(3)
local VirtualUser=game:service'VirtualUser'
game:service('Players').LocalPlayer.Idled:connect(function()
VirtualUser:CaptureController()
VirtualUser:ClickButton2(Vector2.new())
end)
